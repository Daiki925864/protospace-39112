<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <div>こんにちは、</div>
        <%= link_to "#{current_user.name}さん", root_path, class: :greeting__link %>
      </div>
    <% end %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%= render partial: "prototype", locals: { collection: @prototype } %>
    </div>
  </div>
</main>
